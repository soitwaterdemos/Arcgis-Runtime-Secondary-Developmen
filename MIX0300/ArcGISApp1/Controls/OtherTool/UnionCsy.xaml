<UserControl x:Class="ArcGISApp1.Controls.OtherTool.UnionCsy"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ArcGISApp1.Controls.OtherTool"
             mc:Ignorable="d" 
             d:DesignHeight="35" d:DesignWidth="160" ToolTip="合并工具" x:Name="myUserControl" Loaded="myUserControl_Loaded" >
    <WrapPanel Background="#FFE6F4F2">
        <Button x:Name="symbolBtn" Content="样式设置" Height="20" Width="65" Padding="0" Margin="5,2,0,0" Click="symbolBtn_Click"/>
        <Button x:Name="unionBtn" Content="合并要素" Height="20" Width="65" Padding="0" Margin="10,2,0,0" Click="unionBtn_Click"/>
    </WrapPanel>
</UserControl>
